{
    "name": "unborn",
    "version": "0.1.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "deno run --allow-read --allow-env --allow-net --allow-sys --unstable-worker-options --import-map=import_map.json src/index.js",
        "docker:build": "docker build -t unborn . && docker run -p 49160:8080 -d unborn",
        "test": "deno test"
    },
    "author": "PashaYakubovsky",
    "license": "ISC",
    "description": "unborn is multiplatfrom bot for discord and telegram",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.32.1",
        "@google-cloud/storage": "^7.14.0",
        "@slack/bolt": "^4.1.0",
        "crypto": "^1.0.1",
        "discord.js": "^14.16.3",
        "node-cron": "^3.0.3",
        "officeparser": "^5.0.0",
        "openai": "^4.71.1",
        "pdfjs-dist": "^4.8.69",
        "puppeteer": "^23.8.0",
        "sqlite3": "^5.1.7",
        "telegraf": "^4.16.3"
    },
    "compilerOptions": {
        "allowJs": true,
        "lib": [
            "deno.window",
            "deno.worker"
        ],
        "strict": true
    },
    "importMap": "import_map.json",
    "tasks": {
        "start": "deno run --allow-net --allow-read --allow-worker src/index.js"
    }
}
